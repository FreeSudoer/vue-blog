{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],
  "plugins": [
    "jsdoc"
  ],
  "globals": {
    "vue": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-var": 2,
    "one-var": 1,
    "block-scoped-var": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-eval": 1,
    "no-irregular-whitespace": 2,
    "no-script-url": 0,
    "eqeqeq": 2,
    "indent": [2, 2],
    "prefer-const": 0,
    "prefer-spread": 0,
    "space-after-keywords": [0, "always"]
  }
}
